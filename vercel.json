{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "server.js"
        }
    ],
    "crons": [
        {
            "path": "/src/crons/refreshGoogleToken",
            "schedule": "* 5 * * *"
        }
    ]
}